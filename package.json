{
  "name": "component-collection-example",
  "version": "1.1.0",
  "description": "Example for how to build an collection of react components",
  "main": "./lib",
  "author": "ChDeinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ChDeinert/component-collection-example.git"
  },
  "scripts": {
    "dist": "rm -rf ./lib && babel ./src --out-dir ./lib --source-maps --plugins babel-plugin-add-module-exports --ignore \"**/story.js,**/*test.js\"",
    "start": "start-storybook -p 8080",
    "test": "jest",
    "prettify": "prettier --single-quote --tab-width 2 --trailing-comma es5 --write \"src/**/*.js\" "
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^19.0.2",
    "prettier": "^1.3.1",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-test-renderer": "^15.5.4"
  },
  "peerDependencies": {
    "react": "15.5.0",
    "react-dom": "15.5.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  }
}
